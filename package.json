{
  "name": "csa-frontend",
  "version": "1.0.5",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "dev": "concurrently \"tailwindcss --input input.css --output tailwind.css --no-autoprefixer --watch\" \"tailwind-rn --watch\" \"expo start --web\" \"node proxy.js\"",
    "build:tailwind": "tailwindcss --input input.css --output tailwind.css --no-autoprefixer && tailwind-rn",
    "dev:tailwind": "concurrently \"tailwindcss --input input.css --output tailwind.css --no-autoprefixer --watch\" \"tailwind-rn --watch\"",
    "build:web": "expo export --platform web",
    "build:android": "eas build --platform android --local",
    "serve": "cross-env SERVER_ID=58119 SERVE_LOCAL_NETWORK=1 node serve.js",
    "serve:prod": "node serve.js",
    "eas-build-pre-install": "npm config set legacy-peer-deps true",
    "lint": "eslint ."
  },
  "dependencies": {
    "@expo/match-media": "^0.4.0",
    "@expo/webpack-config": "^19.0.0",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-native-picker/picker": "2.4.10",
    "@rneui/base": "^4.0.0-rc.7",
    "@rneui/themed": "^4.0.0-rc.7",
    "email-validator": "^2.0.4",
    "expo": "~49.0.6",
    "expo-calendar": "~11.3.2",
    "expo-checkbox": "~2.4.0",
    "expo-constants": "~14.4.2",
    "expo-image": "~1.3.5",
    "expo-linking": "~5.0.2",
    "expo-mail-composer": "~12.3.0",
    "expo-print": "~12.4.2",
    "expo-router": "^2.0.0",
    "expo-screen-orientation": "~6.0.6",
    "expo-sharing": "~11.5.0",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "jwt-decode": "^3.1.2",
    "pullstate": "^1.25.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.6",
    "react-native-draggable-flatlist": "^4.0.1",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-modal": "^13.0.1",
    "react-native-paper": "^5.10.1",
    "react-native-paper-dates": "^0.18.12",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-vector-icons": "^9.2.0",
    "react-native-web": "~0.19.6",
    "react-native-web-hooks": "^3.0.2",
    "react-responsive": "^9.0.2",
    "tailwind-rn": "^4.2.0",
    "twrnc": "^3.6.0",
    "weeknumber": "^1.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-transform-export-namespace-from": "^7.22.11",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/react": "~18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-native": "^4.1.0",
    "postcss": "^8.4.21",
    "serve-handler": "^6.1.5",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2"
  },
  "private": true
}
